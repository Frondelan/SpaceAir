{
  "expo": {
    "name": "Space Air",
    "slug": "SpaceAir",
    "version": "3.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon/Icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splashscreen/spaceAir.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.sergiorj.spaceair",
      "versionCode": 3,
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
